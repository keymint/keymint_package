<?xml version="1.0" encoding="UTF-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://example.com/ns/collection/keymint_package.xsd"
    format="1">
    <name>foo/bar</name>
    <permissions format="keymint_ros2_dds">
        <issuer_name>permissions_ca</issuer_name>
        <permission>
          <path>permissions1.xml</path>
        </permission>
        <permission>
          <path>permissions2.xml</path>
        </permission>
    </permissions>
    <governances format="keymint_ros2_dds">
        <issuer_name>permissions_ca</issuer_name>
        <governance>
          <path>governance1.xml</path>
        </governance>
        <governance>
          <path>governance2.xml</path>
        </governance>
    </governances>
    <identities format="keymint_ros2_dds">
        <identity>
          <path>identities.xml</path>
        </identity>
    </identities>
    <export>
        <build_type>keymint_ros2_dds</build_type>
    </export>
</package>
